<view class="total">
  <view wx:for="{{actionList}}"
   wx:key="img" 
   class="item" 
   bindtap="go" 
   data-action="{{item.content}}">
    <view class="left">
      <image src="{{item.img}}" class="img"></image>
      <text>{{item.content}}</text>
    </view>
    <view class="right">></view>
  </view>
</view>